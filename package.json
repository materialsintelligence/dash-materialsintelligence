{
  "name": "dash-materialsintelligence",
  "version": "0.0.7",
  "description": "Dash UI component suite for materialsintelligence project",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtshitoyan/dash-materialsintelligence.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtshitoyan/dash-materialsintelligence/issues"
  },
  "homepage": "https://github.com/vtshitoyan/dash-materialsintelligence",
  "scripts": {
    "copy-lib": "copyfiles -u 1 lib/* dash_materialsintelligence",
    "demo": "builder run demo",
    "install-local": "npm run copy-lib && python setup.py install",
    "prepublish": "npm test && builder run build-dist && npm run copy-lib",
    "publish-all": "npm publish && python setup.py sdist upload",
    "publish-pypi": "npm run prepublish && python setup.py sdist upload",
    "start": "builder run build-dev",
    "test": "builder run check",
    "test-watch": "builder run test-frontend-watch",
    "test-debug": "builder run test-frontend-debug",
    "uninstall-local": "pip uninstall dash-materialsintelligence -y"
  },
  "dependencies": {
    "builder": "3.2.2",
    "console-log-level": "^1.4.0",
    "copyfiles": "^1.2.0",
    "dash-components-archetype": "^0.2.11",
    "immutability-helper": "^2.6.5",
    "is-touch-device": "^1.0.1",
    "npm": "^5.10.0",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-select": "^1.2.1"
  },
  "devDependencies": {
    "dash-components-archetype-dev": "^0.2.11",
    "enzyme": "^2.8.2",
    "react-test-renderer": "^15.5.4"
  }
}
